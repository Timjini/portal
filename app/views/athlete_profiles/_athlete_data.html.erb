    <turbo-frame id="accordion-frame">
    <%@levels.inspect%>
    <%=@athlete.athlete_level.inspect%>
        <div id="accordion-collapse" data-accordion="collapse" class="">
                    <!-- LEVEL -->
                    <% @levels.each do |level|%>
                    <div class="general_card mb-4">
                        <h2 id="accordion-collapse-heading-<%=level.id%>">
                            <button type="button" class="border flex items-center justify-between w-full p-5 font-medium text-left rounded-t-xl " data-accordion-target="#accordion-collapse-body-<%=level.id%>" aria-expanded="true" aria-controls="accordion-collapse-body-<%=level.id%>">
                                <span class="" style="font-size:2rem;"><%= level.title%></span>
                                <svg data-accordion-icon class="w-3 h-3 rotate-180 shrink-0" aria-hidden="true" xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 10 6">
                                <path stroke="currentColor" stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 5 5 1 1 5"/>
                                </svg>
                            </button>
                        </h2>
                        <div id="accordion-collapse-body-<%=level.id%>" class="hidden" aria-labelledby="accordion-collapse-heading-<%=level.id%>">
                            <div class="p-5 border border-b-0 ">
                        <%= render 'athlete_profiles/check_list' , level: level %>
                            </div>
                            <div class="add-kpi-form border">
                                <form class="mt-2 mb-3 add-kpi-form">
                                <label for="chat" class="sr-only">Your message</label>
                                <div class="flex items-center  py-2 rounded-lg ">
                                    <textarea id="chat" rows="1" class="block mx-4 p-2.5 w-full text-sm  rounded-lg border bg-gray-50 border border-gray-300 text-gray-900 text-sm rounded-lg focus:ring-blue-500 focus:border-blue-500 block w-full p-2.5" placeholder="Your message..."></textarea>
                                    <button type="submit" class="inline-flex justify-center p-2  rounded-full cursor-pointer ">
                                        <svg class="w-5 h-5 rotate-90" aria-hidden="true" xmlns="http://www.w3.org/2000/svg" fill="currentColor" viewBox="0 0 18 20">
                                            <path d="m17.914 18.594-8-18a1 1 0 0 0-1.828 0l-8 18a1 1 0 0 0 1.157 1.376L8 18.281V9a1 1 0 0 1 2 0v9.281l6.758 1.689a1 1 0 0 0 1.156-1.376Z"></path>
                                        </svg>
                                        <span class="sr-only">Send message</span>
                                    </button>
                                </div>
                                </form>
                            </div>
                            <div>
                            </div>
                        </div>
                    </div>
                    <%end%>
                    
        </div>
    </turbo-frame>
